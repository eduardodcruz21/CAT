<?xml version="1.0" encoding="UTF-8"?>
<webservices xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/javaee_web_services_1_2.xsd" version="1.2">
	<webservice-description>
		<webservice-description-name>CAATService</webservice-description-name>
		<wsdl-file>WEB-INF/wsdl/CAATService.wsdl</wsdl-file>
		<port-component>
			<port-component-name>CAATDelegate</port-component-name>
			<wsdl-service xmlns:pfx="http://sat.gob.mx/comercioexterior/caat/">
			pfx:CAATService</wsdl-service>
			<wsdl-port xmlns:pfx="http://sat.gob.mx/comercioexterior/caat/">
			pfx:CAATPort</wsdl-port>
			<enable-mtom>false</enable-mtom>
			<service-impl-bean>
				<servlet-link>
				mx.gob.sat.comercioexterior.servicio.CAATDelegate</servlet-link>
			</service-impl-bean>
		</port-component>
	</webservice-description>
</webservices>
